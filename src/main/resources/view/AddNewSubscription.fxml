                  <Cursor fx:constant="HAND" />
               </cursor>
            </ComboBox>
            <CheckComboBox fx:id="exercicePlanCCB" layoutX="124.0" layoutY="224.0" prefHeight="34.0" prefWidth="225.0" showCheckedCount="true" style="-fx-: white;" title="Select Exercice Plan">
