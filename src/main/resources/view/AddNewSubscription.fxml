                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <Label layoutX="124.0" layoutY="123.0" onMouseClicked="#activateC" prefHeight="34.0" prefWidth="225.0">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
