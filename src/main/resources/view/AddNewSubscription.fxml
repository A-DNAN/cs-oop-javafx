               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <ComboBox fx:id="trainerCB" layoutX="124.0" layoutY="174.0" onAction="#activateEP" prefHeight="34.0" prefWidth="225.0" promptText="Select Trainer">
